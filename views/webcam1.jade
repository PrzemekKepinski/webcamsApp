extends index

block content

    img(id = "webcam1" src= "http://localhost:3000/images/webcam1-2.jpg")
    h1= title
    p Welcome to #{title}
    script.
        setInterval ( function () {

            var webcam1 = document.getElementById("webcam1");

            if ( webcam1.src === "http://localhost:3000/images/webcam1-2.jpg")
                    webcam1.src = "http://localhost:3000/images/webcam1-3.jpg"
                        else
                            webcam1.src = "http://localhost:3000/images/webcam1-2.jpg"},
                                 5000);


